{% extends 'base.html.twig' %}

{% block title %}SmsMessage{% endblock %}

{% block body %}
    <h1>SmsMessage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sms_message.id }}</td>
            </tr>
            <tr>
                <th>Sender</th>
                <td>{{ sms_message.sender }}</td>
            </tr>
            <tr>
                <th>Receiver</th>
                <td>{{ sms_message.receiver }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ sms_message.message }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_sms_message_index') }}">back to list</a>

    <a href="{{ path('app_sms_message_edit', {'id': sms_message.id}) }}">edit</a>

    {{ include('sms_message/_delete_form.html.twig') }}
{% endblock %}
